<h1><%= @report.name %></h1>

<%= form_tag '', :method => :get do %>
    <% @report.params.each do |param| %>
        <div>
            <%= label_tag param.name %>
            <%= report_param( param ) %>
        </div>
    <% end %>
    <%= submit_tag( t( :report, :scope => :report_cat ) ) %>
<% end %>